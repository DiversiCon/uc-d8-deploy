{
    "name": "ifthen/uccqe",
    "description": "Chicago Quantum Exchange - Custom Upstream",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "require": {
        "php": ">=7.2.0",
        "ext-json": "*",
        "acquia/blt": "^11.4",
        "composer/installers": "^1.0.20",
        "cweagans/composer-patches": "^1.0",
        "drupal/address": "^1.4",
        "drupal/adminimal_admin_toolbar": "^1.7",
        "drupal/adminimal_theme": "^1.3",
        "drupal/allowed_formats": "^1.1",
        "drupal/auto_entityqueue": "^1.0",
        "drupal/autologout": "^1.0",
        "drupal/backup_migrate": "^4.0",
        "drupal/chosen": "^2.6",
        "drupal/config_direct_save": "^1.0",
        "drupal/config_ignore": "^2.1",
        "drupal/config_installer": "^1.0",
        "drupal/config_pages": "^2.2",
        "drupal/config_readonly": "^1.0@beta",
        "drupal/config_split": "^1.3",
        "drupal/config_suite": "^1.2",
        "drupal/console": "^1.9",
        "drupal/consumer_image_styles": "^3.0@RC",
        "drupal/consumers": "^1.9",
        "drupal/core-composer-scaffold": "^8.8",
        "drupal/core-recommended": "^8.8",
        "drupal/decoupled_router": "^1.7",
        "drupal/devel": "^2.1",
        "drupal/draggableviews": "^1.0",
        "drupal/dropzonejs": "^2.0@alpha",
        "drupal/eck": "1.0-alpha4",
        "drupal/entity_browser": "^2",
        "drupal/entity_clone": "1.x-dev#9265e35",
        "drupal/entityqueue": "^1.0@alpha",
        "drupal/field_group": "^3.0",
        "drupal/flood_unblock": "^1.5",
        "drupal/focal_point": "^1.0@beta",
        "drupal/google_tag": "^1.1",
        "drupal/image_widget_crop": "^2.2",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/jsonapi_extras": "^3.3",
        "drupal/link_attributes": "^1.3",
        "drupal/linkit": "^4.3",
        "drupal/mailsystem": "^4.3",
        "drupal/masquerade": "^2.0@beta",
        "drupal/media_entity_browser": "^2.0",
        "drupal/migrate_file": "^1.1",
        "drupal/migrate_plus": "^4.1",
        "drupal/migrate_tools": "^4.1",
        "drupal/migrate_upgrade": "^2.0",
        "drupal/name": "^1.0@RC",
        "drupal/paragraphs": "^1.3",
        "drupal/pathauto": "^1.2",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/rebuild_cache_access": "^1.4",
        "drupal/recaptcha": "^2.3",
        "drupal/redirect": "^1.2",
        "drupal/restui": "^1.17",
        "drupal/robotstxt": "^1.2",
        "drupal/role_based_theme_switcher": "^1.0",
        "drupal/schema_metatag": "^1.3",
        "drupal/search_api": "^1.10",
        "drupal/search_api_solr": "1.2",
        "drupal/smart_trim": "^1.1",
        "drupal/simple_sitemap": "^3.5",
        "drupal/smtp": "dev-1.x",
        "drupal/stage_file_proxy": "^1.0@alpha",
        "drupal/token_custom": "^1.0@alpha",
        "drupal/twig_field_value": "^1.1",
        "drupal/twig_tweak": "^2.1",
        "drupal/views_bulk_operations": "^2.4",
        "drupal/webform": "^5.2",
        "drush/drush": "~10.2",
        "enyo/dropzone": "^4.3",
        "flow/jsonpath": "^0.4.0",
        "harvesthq/chosen": "^1.8",
        "pantheon-systems/drupal-integrations": "^8",
        "pantheon-systems/quicksilver-pushback": "^2",
        "pheromone/phpcs-security-audit": "^2.0",
        "rvtraveller/qs-composer-installer": "^1.1",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "acquia/blt-require-dev": "^11.0.0-alpha1",
        "behat/behat": "3.*",
        "behat/mink": "^1.7",
        "behat/mink-extension": "^2.2",
        "behat/mink-goutte-driver": "^1.2",
        "behat/mink-selenium2-driver": "^1.3.1",
        "bex/behat-screenshot": "^1.2",
        "dmore/behat-chrome-extension": "^1.3",
        "drupal/coder": "^8.3.1",
        "drupal/drupal-extension": "~3.2",
        "geerlingguy/drupal-vm": "^4.8",
        "genesis/behat-fail-aid": "^2.1",
        "jarnaiz/behat-junit-formatter": "^1.3.2",
        "jcalderonzumba/gastonjs": "^1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "^0.3.1",
        "mikey179/vfsstream": "~1.2",
        "sensiolabs-de/deprecation-detector": "0.1.0-alpha4",
        "squizlabs/php_codesniffer": "^3.4.0",
        "symfony/phpunit-bridge": "^3.4.3"
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "psr-4": {
            "Acquia\\": "blt/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\PHPUnit\\": "tests/phpunit/src/"
        }
    },
    "scripts": {
        "build-assets": [
            "@prepare-for-pantheon",
            "composer install --optimize-autoloader --no-dev"
        ],
        "lint": "find web/modules/custom web/themes/custom -name '*.php' -exec php -l {} \\;",
        "code-sniff": [
            "./vendor/bin/phpcs --standard=Drupal --extensions=php,module,inc,install,test,profile,theme,css,info,txt,md --ignore=node_modules,bower_components,vendor ./web/modules/custom",
            "./vendor/bin/phpcs --standard=Drupal --extensions=php,module,inc,install,test,profile,theme,css,info,txt,md --ignore=node_modules,bower_components,vendor ./web/themes/custom",
            "./vendor/bin/phpcs --standard=DrupalPractice --extensions=php,module,inc,install,test,profile,theme,css,info,txt,md --ignore=node_modules,bower_components,vendor ./web/modules/custom",
            "./vendor/bin/phpcs --standard=DrupalPractice --extensions=php,module,inc,install,test,profile,theme,css,info,txt,md --ignore=node_modules,bower_components,vendor ./web/themes/custom"
        ],
        "unit-test": "echo 'No unit test step defined.'",
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "prepare-for-pantheon": "DrupalProject\\composer\\ScriptHandler::prepareForPantheon",
        "post-install-cmd": [
            "@drupal-scaffold",
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-create-project-cmd": [
            "@drupal-scaffold",
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "nuke": [
            "rm -rf vendor composer.lock docroot/core docroot/modules/contrib docroot/profiles/contrib docroot/themes/contrib",
            "@composer clearcache --ansi",
            "@composer install --ansi"
        ]

    },
    "extra": {
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "docroot/core": ["type:drupal-core"],
            "docroot/modules/contrib/{$name}": ["type:drupal-module"],
            "docroot/modules/custom/{$name}": ["type:drupal-custom-module"],
            "docroot/profiles/contrib/{$name}": ["type:drupal-profile"],
            "docroot/profiles/custom/{$name}": ["type:drupal-custom-profile"],
            "docroot/themes/contrib/{$name}": ["type:drupal-theme"],
            "docroot/themes/custom/{$name}": ["type:drupal-custom-theme"],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "build-env": {
            "install-cms": [
                "drush site-install standard --account-mail={account-mail} --account-name={account-name} --account-pass={account-pass} --site-mail={site-mail} --site-name={site-name} --yes"
            ],
            "export-configuration": "drush config-export --yes"
        },
        "drupal-scaffold": {
            "allowed-packages": [
                "pantheon-systems/drupal-integrations"
            ],
            "locations": {
                "web-root": "./docroot"
            },
            "file-mapping": {
                "[web-root]/sites/default/default.services.yml": {
                    "mode": "replace",
                    "path": "docroot/core/assets/scaffold/files/default.services.yml",
                    "overwrite": false
                },
                "[web-root]/sites/default/default.settings.php": {
                    "mode": "replace",
                    "path": "docroot/core/assets/scaffold/files/default.settings.php",
                    "overwrite": false
                }
            }
        },
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/acquia_connector": {
                "Allow uninstall of acquia_search module.": "patches/allow-uninstall-of-acquia-search.patch"
            },
            "drupal/core": {
                "Disabled update module shouldn't produce a status report warning, https://www.drupal.org/project/drupal/issues/2869592#comment-12097889": "https://www.drupal.org/files/issues/2869592-remove-update-warning-7.patch",
                "views theme hooks, https://www.drupal.org/project/drupal/issues/2923634#comment-12561788": "https://www.drupal.org/files/issues/2018-04-07/2923634-35.patch",
                "taxonomy theme hooks, https://www.drupal.org/project/drupal/issues/2767243#comment-12285508": "https://www.drupal.org/files/issues/core-theme-suggestion-for-taxonomy-view-modes-2767243-14.patch",
                "Avoid error when $options is NULL in buildUrl(), https://www.drupal.org/project/drupal/issues/2871217": "https://www.drupal.org/files/issues/2871217-null-options-url-11.patch",
                "Array merge failure on line 153 of core/lib/Drupal/Core/Utility/LinkGenerator.php, https://www.drupal.org/project/drupal/issues/2596937": "https://www.drupal.org/files/issues/array_merge_failure_on-2596937-65.patch"
            },
            "drupal/entity_clone": {
                "https://www.drupal.org/node/2706639 => 2800203 - Event dispatcher for clone events": "https://www.drupal.org/files/issues/entity_clone-event_dispatcher-2800203-5.patch",
                "2706639 - Support for cloning nodes that have another entities attached": "https://www.drupal.org/files/issues/entity_clone-attached_entities-2706639-25.patch",
                "2769823 - Getting error after clicking on abort clone button": "https://www.drupal.org/files/issues/entity_clone-abort_clone_causes_fatal_error-2769823-4.patch"
            },
            "drupal/migrate_plus": {
                "3007709 - Add XPath-style filtering ability in JSON data parser plugin": "https://www.drupal.org/files/issues/2018-11-14/migrate_plus-json-xpath-filtering-3007709-8.patch"
            }
        },
        "merge-plugin": {
            "require": [
                "blt/composer.required.json"
            ],
            "merge-extra": true,
            "merge-extra-deep": true,
            "merge-scripts": true,
            "replace": false,
            "ignore-duplicates": true,
            "include": [
                "docroot/modules/custom/*/composer.json"
            ],
            "recurse": true
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "7.2.0"
        }
    }
}
